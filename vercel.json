{
  "rewrites": [
    {
      "source": "/api/.*",
      "destination": "https://barsinshaat-backend.vercel.app/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/.*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE"
        }
      ]
    }
  ]
}
